{
  "name": "ui-submodules-loader",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "npm-run-all": "^4",
    "lerna": "^3",
    "rimraf": "^3",
    "prettier": "^2",
    "typescript": "^4",
    "connected-react-router": "^6"
  },
  "resolutions": {
    "webpack": "4.42.0"
  },
  "scripts": {
    "clear": "lerna run clear",
    "clean": "yarn run clear && rimraf ./packages/*/node_modules && rimraf ./node_modules",
    "build": "lerna run build --stream",
    "build-esm": "lerna run build-esm --stream",
    "watch": "lerna run watch --parallel",
    "watch:lib": "lerna run watch:lib --parallel",
    "release": "lerna run release  --parallel",
    "start:web": "react-scripts start",
    "run-web": "cd examples/run-demo && yarn -s web",
    "run-web-watch": "npm-run-all --parallel watch run-web",
    "start": "npm-run-all --parallel watch start:web",
    "test": "react-scripts test"
  },
  "workspaces": [
    "e2e/*",
    "modules/*",
    "examples/*",
    "packages/*"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "react-redux": "^7"
  }
}
