    "@material-ui/lab": "^4",
    "@types/lodash": "^4",
    "@dgtx/ui-core": "^0",
    "@dgtx/ui-scl": "^0",
    "@dgtx/ui-utils": "^0"
  },
  "devDependencies": {
    "rimraf": "^3"
  },
