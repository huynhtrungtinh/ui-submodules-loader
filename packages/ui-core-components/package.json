    "@material-ui/system": "^4",
    "@material-ui/types": "^5",
    "@material-ui/utils": "^4",
    "clsx": "^1",
    "react": "^16"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.155",
    "lodash": "^4.17.15",
