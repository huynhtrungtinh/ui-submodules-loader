    "@types/react-redux-i18n": "0.0.11",
    "redux-devtools-extension": "^2.13.8",
    "axios": "^0.18.0",
    "clsx": "^1.1.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "react-draggable": "^4.1.0",
    "react-virtualized": "^9.*",
    "@material-ui/styles": "^4",
    "@material-ui/system": "^4",
    "@material-ui/types": "^5",
    "@material-ui/utils": "^4",
